# LLM Configuration
# Choose your preferred LLM provider and configure accordingly

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.7

# Qwen (Alibaba Cloud)
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL=qwen-plus
QWEN_TEMPERATURE=0.7

# MiniMax
MINMAX_API_KEY=your_minmax_api_key_here
MINMAX_MODEL=MiniMax-M2.1
MINMAX_TEMPERATURE=0.7

# Search Tools
# Tavily is required for web search functionality
TAVILY_API_KEY=your_tavily_api_key_here

# Optional: Other search providers
SERP_API_KEY=your_serp_api_key_here
SERP_ENABLED=false

SEARXNG_URL=http://localhost:8080
SEARXNG_ENABLED=false

WOLFRAM_APP_ID=your_wolfram_app_id_here

# Search Configuration
MAX_SUB_QUESTIONS=5
MAX_SEARCH_RESULTS=5
SEARCH_TIMEOUT=10

# Scraping Configuration
SCRAPING_STRATEGY=crawl4ai
MAX_SCRAPE_PAGES=5
SCRAPE_TIMEOUT=30

# Reranker Configuration
RERANKER_MODEL=jina
JINA_API_KEY=your_jina_api_key_here
INFINITY_API_URL=http://localhost:7997

# Development
# DEBUG: Set to true to enable debug logging (shows all logger.debug() messages)
# LOG_LEVEL: Logging level when DEBUG=false (DEBUG, INFO, WARNING, ERROR, CRITICAL)
DEBUG=false
LOG_LEVEL=INFO
